<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd" id="WebApp_ID" version="2.4">
  <display-name>CollegeBook</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>StoreControllerServlet</servlet-name>
    <servlet-class>com.riseon.chitfund.controller.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StoreControllerServlet</servlet-name>
    <url-pattern>/loginurl</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ChangePassword</servlet-name>
    <servlet-class>com.riseon.chitfund.controller.ChangePassword</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ChangePassword</servlet-name>
    <url-pattern>/processChangePWD</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserLogin</servlet-name>
    <servlet-class>com.riseon.chitfund.controller.UserLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserLogin</servlet-name>
    <url-pattern>/userlogin</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>CreateAgent</servlet-name>
    <servlet-class>com.riseon.chitfund.controller.CreateNewAgent</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreateAgent</servlet-name>
    <url-pattern>/CreateAgent</url-pattern>
  </servlet-mapping>
</web-app>